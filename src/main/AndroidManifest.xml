        <activity
            android:name=".ui.va.mandiri.MandiriVaActivity"
            android:theme="@style/AppThemeFullScreen">
        </activity>

        <activity
            android:name=".ui.va.mandirisyariah.MandiriSyariahVaActivity"
            android:theme="@style/AppThemeFullScreen">
        </activity>

    </application>
</manifest>