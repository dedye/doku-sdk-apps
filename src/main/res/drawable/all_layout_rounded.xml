<?xml version="1.0" encoding="utf-8"?>
<shape xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="rectangle">
    <solid android:color="#FFE7D4" />
    <stroke
        android:width="1dp"
        android:color="#FFE7D4" />
    <padding
        android:bottom="5dp"
        android:left="5dp"
        android:right="5dp"
        android:top="5dp" />
    <corners
        android:radius="11dp"
        />
    <gradient
        android:angle="90"
        android:centerColor="#FFE7D4"
        android:centerY="0.1"
        android:endColor="#FFE7D4"
        android:startColor="#FFE7D4"
        android:type="linear" />
</shape>